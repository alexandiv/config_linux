{
    "security.workspace.trust.enabled": false,
    "workbench.startupEditor": "none",
    "window.zoomLevel": 0.6,
    "editor.fontFamily": "Monaco, Menlo, 'JetBrainsMono NFM', monospace",
    "editor.fontSize": 13,
    "editor.lineHeight": 19,
    "editor.fontLigatures": true,
    "editor.cursorStyle": "block",
    "editor.minimap.enabled": false,
    "editor.matchBrackets": "near",
    "editor.bracketPairColorization.enabled": false,
    "editor.unicodeHighlight.nonBasicASCII": false,
    // "editor.quickSuggestions": {
    //     "other": "off",
    //     "comments": "off",
    //     "strings": "off"
    // },
    // "editor.hover.enabled": false,
    // "editor.parameterHints.enabled": false,
    // "editor.acceptSuggestionOnCommitCharacter": false,
    "terminal.integrated.fontFamily": "'JetBrainsMono NFM', Menlo, 'SF Mono', Monaco, 'Courier New', monospace",
    "terminal.integrated.fontSize": 13,
    "debug.console.fontFamily": "'JetBrainsMono NFM', Menlo, 'SF Mono', Monaco, 'Courier New', monospace",
    "debug.console.fontSize": 13,
    "cmake.configureOnOpen": false,
    "cmake.debugConfig": {
        "cwd": "${workspaceFolder}"
    },
    "cmake.parallelJobs": 1,
    "C_Cpp.clang_format_style": "Visual Studio"
    // "files.encoding": "windows1251"
}
